{
    "name": "portex",
    "displayName": "PortEx",
    "description": "Export and import extensions quickly. No GitHub tokens required.",
    "version": "0.0.1",
    "publisher": "kronosfear",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.export"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.export",
                "title": "Export VSCode Extensions"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "hastebin-gen": "^1.3.1",
        "shelljs": "^0.8.2",
        "url-parse": "^1.4.3"
    }
}
